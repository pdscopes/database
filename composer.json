{
    "name": "madesimple/database",
    "description": "Database entities and query builder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Scopes",
            "email": "peter.scopes@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "psr/log": "~1.0",
        "madesimple/php-arrays": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "mockery/mockery": "^1",
        "fzaninotto/faker": "^1",
        "symfony/console": "^5",
        "symfony/filesystem": "^5",
        "symfony/finder": "^5",
        "monolog/monolog": "^2"
    },
    "autoload": {
        "psr-4": {
            "MadeSimple\\Database\\": "src/"
        }
    },
    "suggest": {
        "fzaninotto/faker": "Generates fake data for you (^1).",
        "symfony/console": "Required for Symfony commands (^5)",
        "symfony/filesystem": "Required for Symfony commands (^5)",
        "symfony/finder": "Required for Symfony commands (^5)"
    },
    "autoload-dev": {
        "psr-4": {
            "MadeSimple\\Database\\Tests\\": "tests/",
            "MadeSimple\\Database\\Example\\": "examples/"
        }
    },
    "bin": [
        "bin/database"
    ]
}
